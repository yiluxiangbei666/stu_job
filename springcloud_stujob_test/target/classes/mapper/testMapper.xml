<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.dao.testdao">
    <insert id="inserttest" parameterType="test.bean.test">
        insert into test(test_id,test_user_id,test_company_name,position,test_content_xml_url,test_send_time)
        values(#{test_id},#{test_user_id},#{test_company_name},#{position},#{test_content_xml_url},#{test_send_time})
    </insert>
</mapper>