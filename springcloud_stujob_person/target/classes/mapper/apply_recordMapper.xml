<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="person.dao.apply_recorddao">
    <select id="getmyapply_record" parameterType="java.lang.String" resultType="person.bean.apply_record">
        select * from apply_record where apply_record_phone=#{phone}
    </select>
</mapper>